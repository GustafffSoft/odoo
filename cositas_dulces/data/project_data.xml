<odoo>
    <data noupdate="1">
        <!-- Crear el proyecto 'Cositas Dulces' -->
        <record id="project_cositas_dulces" model="project.project">
            <field name="name">Cositas Dulces</field>
            <field name="is_favorite">True</field>
            <!-- Otros campos necesarios para configurar tu proyecto -->
        </record>

        <!-- Crear una etapa (grupo) dentro del proyecto 'Cositas Dulces' -->
        <record id="project_task_stage_cositas_dulces" model="project.task.type">
            <field name="name">Grupo Cositas Dulces</field>
                <field name="project_ids" eval="[(4, ref('project_cositas_dulces'))]"/>
            <!-- Otros campos necesarios para la etapa -->
        </record>
    </data>
</odoo>