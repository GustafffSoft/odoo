<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="licencias" model="ir.ui.view">
            <field name="name">Licencias</field>
            <field name="model">hr.leave</field>
            <field name="priority" eval="2"/>
            <field name="arch" type="xml">
                <calendar string="Licencias" create="0" mode="month" date_start="date_from" date_stop="date_to"
                          hide_time="true"
                          color="employee_id">
                    <field name="employee_id"/>
                    <!--                    <field name="birthday"/>-->
                </calendar>
            </field>
        </record>

        <record model="ir.actions.act_window" id="action_licencias">
            <field name="name">Licencias</field>
            <field name="res_model">hr.leave</field>
            <field name="view_mode">calendar,search</field>
        </record>

    </data>
</odoo>